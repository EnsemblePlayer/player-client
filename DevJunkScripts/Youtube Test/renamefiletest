import vlc
import sys
import youtube_dl
import os

def down(a):
	b="https://www.youtube.com/"+a
	c=a[8:]
	print(a)
	print(b)
	print(c)
	
	os.rename(c, c+".mp3")	

if __name__ == "__main__":
	down(sys.argv[1])
